    @Html.Partial("_InsertAfter")
    <hr class="docs-separator"/>

    @Html.Partial("_InsertBefore")
    <hr class="docs-separator"/>
    
    @Html.Partial("_Prepend")
    <hr class="docs-separator"/>

    @Html.Partial("_Remove")
    <hr class="docs-separator"/>

    @Html.Partial("_RemoveAttribute")
    <hr class="docs-separator"/>

    @Html.Partial("_Replace")
