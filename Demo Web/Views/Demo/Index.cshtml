    @Html.Partial("_SetAttribute")
    <hr class="docs-separator"/>

    @Html.Partial("_FadeIn")
    <hr class="docs-separator"/>

    @Html.Partial("_FadeOut")
    <hr class="docs-separator"/>
    
    @Html.Partial("_Hide")
    <hr class="docs-separator"/>

    @Html.Partial("_Show")
    <hr class="docs-separator"/>
    
    @Html.Partial("_SlideUp")
    <hr class="docs-separator"/>

    @Html.Partial("_SlideDown")
    <hr class="docs-separator"/>

    @Html.Partial("_AddClass")
    <hr class="docs-separator"/>

    @Html.Partial("_RemoveClass")
    <hr class="docs-separator"/>

